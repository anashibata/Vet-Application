@model Argus.Models.ExameReferencia

<h2>Eliminação</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())        
{    
        @Html.TextBoxFor(model => model.CODIGO_EXAME, new { Type = "hidden" })
        @Html.TextBoxFor(model => model.CODIGO_ITEM, new { Type = "hidden" })
        <table>
        
        <tr>
        <td>@Html.LabelFor(model => model.DESCRICAO)</td>
        <td>@Html.TextBoxFor(model => model.DESCRICAO, new { Readonly = "readonly" }) </td>
        </tr>
         
        </table>
    
        <p> <input type="submit" value="Eliminar" /> </p>
}

<div>
    @Html.ActionLink("Voltar", "Listar")
</div>
